<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 连框颜色值 -->
    <item>
        <shape android:shape="oval">
            <solid android:color="#3850E3C2"/>
            <size android:width="10.7dp"
                  android:height="10.7dp"/>
        </shape>
    </item>
    <!-- 主体背景颜色值 -->
    <item android:left="5dp"
          android:top="5dp"
          android:right="5dp"
          android:bottom="5dp">
        <shape android:shape="oval">
            <solid android:color="#50E3C2"/>
            <size android:width="16dp"
                  android:height="16dp"/>
        </shape>
    </item>
</layer-list>